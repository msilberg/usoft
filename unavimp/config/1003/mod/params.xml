<?xml version="1.0" encoding="UTF-8" ?>
<parameters>
    <!-- Script address -->
    <param name="traddr">http://uadmin.no-ip.biz/</param>
    <!-- Admin address -->
    <param name="raddr2">http://uadmin.no-ip.biz:8080/</param>
    <!-- Image server address -->
    <param name="get_image">http://uadmin.no-ip.biz:8080/getimage/imageid/</param>
    <!-- List slider buttons maximal number per each one pages -->
    <param name="ls_max_num">10</param>
    <!-- Number of banner per one page -->
    <param name="bann_num">14</param>
    <!-- Special categories array -->
    <param name="spec_cat">
        <elem name="bankers">342</elem>
    </param>
    <!-- The top limiter of the highest number of promoted stores -->
    <param name="max_spm">10</param>
    <!-- Drum info rows divide number -->
    <param name="dirdn">19</param>
    <!-- Max subcategories number per each one page -->
    <param name="max_scats">27</param>
    <!-- Max number of columns of subcategories list -->
    <param name="scats_cols">3</param>
    <!-- Max number of rows of subcategories list -->
    <param name="scats_rows">9</param>
    <!-- Max stores number per each one page -->
    <param name="max_stores">30</param>
    <!-- Max number of columns of stores list -->
    <param name="stores_cols">3</param>
    <!-- Max number of rows of stores list -->
    <param name="stores_rows">10</param>
    <!-- Slider cells spacing in the subcategories and stores list -->
    <param name="scs">5</param>
    <!-- Slider cells spacing in the stores and stores list -->
    <param name="scs_st">4</param>
    <!-- Slider cell height -->
    <param name="slch">81</param>
    <!-- Slider cell height for stores list -->
    <param name="slch_st">70</param>
    <!-- New number type -->
    <param name="nn_type">1</param>
    <!-- Max number of search results' lines per each one page -->
    <param name="max_sch">9</param>
    <!-- Max number of columns of search results' list -->
    <param name="sch_cols">1</param>
    <!-- Max number of rows of search results' list -->
    <param name="sch_rows">9</param>
    <!-- Minisite buttons variables: mlt - multiplier, fct = linear factor -->
    <param name="mbtn_vars">
        <elem name="mlt">95</elem>
        <elem name="fct">46</elem>
    </param>
    <!-- Minsite showcase dimensions -->
    <param name="pctup_w">790</param>
    <param name="pctup_h">500</param>
    <!-- Limitation for the number of promoted stores on the stand-by mode -->
    <param name="standby_lim">21</param>
    <!-- Showcase dimensions -->
    <param name="shwcs_dmns">
        <elem name="w">790</elem>
        <elem name="h">500</elem>
    </param>
    <!-- Max number for service icons per one page in the listing -->
    <param name="max_si">24</param>
    <!-- Max number of columns in SI list -->
    <param name="si_cols">6</param>
    <!-- Slider cells spacing in the service icons list -->
    <param name="scs_si">0</param>
    <!-- Cell height in service icons list -->
    <param name="sich">180</param>
</parameters>
